{
  "name": "backpackerdeals-next",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=7",
    "node": ">=14"
  },
  "scripts": {
    "dev": "$npm_execpath run codegen && NODE_OPTIONS='--max_old_space_size=8192 --inspect' next dev",
    "build": "$npm_execpath run codegen && next build",
    "debug": "NODE_ENV=production NODE_OPTIONS='--max_old_space_size=6144 --inspect' next start",
    "start": "NODE_ENV=production next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "schema:download": "rover graph introspect ${TRAVELLO_API:-'https://api-stage.travelloapp.com/graphql'} --header 'x-api-key: test123' --header \"Authorization: Bearer ${TRAVELLO_GRAPH_STAGE_ACCESS_TOKEN}\" > schema.graphql",
    "schema:download:prod": "rover graph introspect ${TRAVELLO_API:-'https://api.travelloapp.com/graphql'} --header 'x-api-key: 82a65efa-cde3-4768-bc2d-4221cc667ba4' --header \"Authorization: Bearer ${TRAVELLO_GRAPH_PRODUCTION_ACCESS_TOKEN}\" > schema.graphql",
    "codegen": "graphql-codegen",
    "clean": "del-cli ./.next && del-cli '**/*/*.generated.ts'",
    "analyze": "ANALYZE=true npm run build",
    "analyze-bundle": "source-map-explorer .next/static/**/*.js",
    "prepare": "node ./prepare.js",
    "test": "NOCK_BACK_MODE=lockdown jest --no-cache --detectOpenHandles --forceExit --coverage --runInBand",
    "test:update": "NOCK_BACK_MODE=update jest --no-cache --detectOpenHandles --forceExit --coverage --runInBand",
    "tdd": "NOCK_BACK_MODE=record node --inspect=0.0.0.0 ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "overrides": {
    "react": "^18.2.0"
  },
